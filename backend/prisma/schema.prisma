generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Mock {
  id            Int     @id @default(autoincrement())
  name          String
  url           String
  method        String
  body_patterns String?
  status_code   Int
  headers       String?
  body          String
}
